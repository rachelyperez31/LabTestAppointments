@using LabTestAppointments.Data.Entities
﻿
@model LabTestAppointments.Data.Entities.MedicalImage

@{
    ViewData["Title"] = "Detalles de img. médica";
}

<h1 class="display-4">Detalles</h1>

<div>
    <h4>Imagen médica</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Nombre
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Name)
        </dd>
        <dt class="col-sm-2">
            Descripción
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Description)
        </dd>
    </dl>
</div>
<div>
    @Html.ActionLink("Editar", "Edit", new { id = Model.Id }) |
    <a asp-action="Index">Regresar al listado</a>
</div>
</div>