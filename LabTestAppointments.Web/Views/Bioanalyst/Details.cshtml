@using LabTestAppointments.Data.Entities
@model LabTestAppointments.Data.Entities.Bioanalyst

@{
    ViewData["Title"] = "Detalles";
}

<h1 class="display-4">Detalles</h1>

<div>
    <h4>Bioanalista</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Nombre
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.FirstName)
        </dd>
        <dt class="col-sm-2">
            Apellido
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.LastName)
        </dd>
    </dl>
</div>
<div>
    @Html.ActionLink("Editar", "Edit", new { id = Model.Id }) |
    <a asp-action="Index">Regresar al listado</a>
</div>