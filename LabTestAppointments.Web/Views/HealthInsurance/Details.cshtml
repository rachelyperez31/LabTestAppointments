@using LabTestAppointments.Data.Entities
@model LabTestAppointments.Data.Entities.HealthInsurance

@{
    ViewData["Title"] = "Detalles";
}

<h1 class="display-4">Detalles</h1>

<div>
    <h4>Seguros médicos</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Plan
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Plan)
        </dd>
        <dt class="col-sm-2">
            Aseguradora
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.InsuranceProvider.Name)
        </dd>
    </dl>
</div>
<div>
    @Html.ActionLink("Editar", "Edit", new { id = Model.Id }) |
    <a asp-action="Index">Regresar al listado</a>
</div>
